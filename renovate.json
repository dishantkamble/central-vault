{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchDatasources": ["github"],
      "registryUrls": ["https://maven.pkg.github.com/dishantkamble/central-java-dependency"]
    }
  ],
  "hostRules": [
   {
      "matchHost": "https://maven.pkg.github.com/dishantkamble/",
      "token": "process.env.GITHUB_TOKEN"
    }
  ]
}
